}
.flex-top,
.flex-top-between {
  align-items: flex-start;
}
.flex-middle,
.flex-center-xy,
.flex-center-between {
  align-items: center;
}
.flex-bottom {
  align-items: flex-end;
}
.flex-stretch {
  align-items: stretch;
}
.flex-nowrap {
  flex-wrap: nowrap;
}
.flex-wrap {
  flex-wrap: wrap;
}
.flex-full {
  flex: 1;
